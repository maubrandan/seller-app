{
    "extends": "./tsconfig.json",
    "compilerOptions": {
      "outDir": "./out-tsc/app",
      "types": [],
      "target": "es2015",
      "module": "es2015",
      "strict": true,
      "baseUrl": "./",
      "paths": {
        "@app/*": ["src/app/*"],
        "@environments/*": ["src/environments/*"]
      }
    },
    "files": [
      "src/main.ts",
      "src/polyfills.ts"
    ],
    "include": [
      "src/**/*.d.ts"
    ]
  }
  